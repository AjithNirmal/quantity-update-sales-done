    <body>
        <table border="1px solid black">
            <tr>
                <th>customer_id</th>
                <th>customer_name</th>
                <th>customer_email</th>

            </tr>
               
            <% for(let i=0;i<customer.length;i++) {%>
                <tr>
                    <td><%= customer[i]._id%></td>
                    <td><%= customer[i].name%></td>
                    <td><%= customer[i].email%></td>
                 </tr>  
            <% } %>
          
    
        </table>

    </body>
</html>